      "HiResBatchCount": 1,
      "DefaultModel": "dreamshaper",
      "SdDefaultModel": "stolendreams",
      "UpscaleMultiplier": 2
    },
    "BotSettings": {
      "JobLimitPerUser": 3,
      "ActiveJobs": 1
    }
  }
}