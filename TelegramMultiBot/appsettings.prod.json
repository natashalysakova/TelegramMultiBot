{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore.*": "Warning"
    }
  },
  "ImageGeneation": {
    "DatabaseCleanupInterval": 3600,
    "JobAge": 86400,
    "RemoveFiles": true,
    "Automatic1111": {
      "PayloadPath": "Configuration/Payload",
